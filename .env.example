# OpenAI (Required)
OPENAI_API_KEY=
REACT_APP_OPENAI_API_KEY=

# Auth0 (Required)
REACT_APP_AUTH0_DOMAIN=
REACT_APP_AUTH0_CLIENT_ID=

# Auth0 (Optional)
REACT_APP_AUTH0_AUDIENCE=
REACT_APP_AUTH0_ROLES_CLAIM=
REACT_APP_AUTH0_ORG_CLAIM=

# Neon PostgreSQL (Document Metadata Persistence)
NEON_DATABASE_URL=
REACT_APP_RAG_DOCS_FUNCTION=/.netlify/functions/rag-documents

# Netlify Blob Storage (Document Store)
# Falls back to NETLIFY_SITE_ID when unset
NETLIFY_BLOBS_SITE_ID=
# Optional fallback used by Netlify CLIs/deploys
NETLIFY_SITE_ID=
# Falls back to NETLIFY_AUTH_TOKEN when unset
NETLIFY_BLOBS_TOKEN=
# Optional fallback used by Netlify CLIs/deploys
NETLIFY_AUTH_TOKEN=
# Falls back to NETLIFY_BLOB_STORE when unset
RAG_BLOB_STORE=
# Optional fallback for legacy configs
NETLIFY_BLOB_STORE=
# Defaults to 'rag-documents' when unset
RAG_BLOB_PREFIX=rag-documents

# Support Requests (Email via SendGrid)
SUPPORT_REQUEST_SENDGRID_API_KEY=
SUPPORT_REQUEST_FROM_EMAIL=
SUPPORT_REQUEST_FROM_NAME="AcceleraQA Support"
SUPPORT_REQUEST_TO_EMAIL=

# Feature Flags (Optional)
REACT_APP_ENABLE_AI_SUGGESTIONS=true
